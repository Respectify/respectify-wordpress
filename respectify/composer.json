{
    "name": "respectify/respectify-wordpress",
    "description": "Wordpress comment evaluation via the Respectify API",
    "type": "wordpress-plugin",
    "require": {
        "respectify/respectify-php": "^0.2",
        "sniccowp/php-scoper-wordpress-excludes": "^6.6"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/vintagedave/respectify-wordpress"
        }
    ],
    "autoload": {
        "psr-4": {
            "Respectify\\": "includes/"
        }
    },
    "require-dev": {
        "humbug/php-scoper": "^0.18.15"
    }
}
